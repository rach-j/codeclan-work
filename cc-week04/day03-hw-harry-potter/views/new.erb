<form  action="/studentregistry" method="post">
  <label for="first-name"> First Name: </label>
  <input id="first-name" type="text" name="first_name" />

  <label for="last-name"> Last Name: </label>
  <input id="last-name" type="text" name="last_name" />

  <label for="house-select"> Select a House: </label>
  <select id="house-select" name="house_id" />
  <option value=1> Gryffindor </option>
  <option value=2> Hufflepuff </option>
  <option value=3> Ravenclaw </option>
  <option value=4> Slytherin </option>
</select>

<label for="age"> Age: </label>
<input id="age" type="text" name="age" />

<input type="submit" name="Enter Student"/>

</form>
